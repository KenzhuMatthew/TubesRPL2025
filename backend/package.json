{
  "name": "siap-bimbingan-backend",
  "version": "1.0.0",
  "description": "Backend API for SIAP Bimbingan - Thesis Guidance Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:create": "psql -U postgres -f database/create-db.sql",
    "db:schema": "psql -U postgres -d siap_bimbingan -f database/schema.sql",
    "db:seed": "psql -U postgres -d siap_bimbingan -f database/seed.sql",
    "db:setup": "npm run db:create && npm run db:schema && npm run db:seed",
    "db:reset": "psql -U postgres -d siap_bimbingan -f database/schema.sql && npm run db:seed",
    "hash-password": "node scripts/hash-password.js"
  },
  "keywords": [
    "thesis",
    "guidance",
    "management",
    "education",
    "postgresql"
  ],
  "author": "UNPAR Informatika",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "socket.io": "^4.6.0",
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}